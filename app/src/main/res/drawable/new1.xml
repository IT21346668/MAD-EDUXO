<?xml version="1.0" encoding="utf-8"?>

    <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
        <item>
            <shape android:shape="rectangle">
                <gradient
                    android:startColor="#B01243E6"

                    android:endColor="#CFD7E8"
                    android:type="linear"
                    android:angle="20"/>
                <corners
                    android:radius="20dp"/>

            </shape>
        </item>
        <item>
            <shape android:shape="rectangle">
                <solid android:color="#00000000"/>
                <corners
                    android:radius="20dp"/>
                <padding
                    android:left="8dp"
                    android:top="8dp"
                    android:right="8dp"
                    android:bottom="8dp"/>
                <stroke
                    android:width="1dp"
                    android:color="#000000"/>
            </shape>
        </item>
    </layer-list>


